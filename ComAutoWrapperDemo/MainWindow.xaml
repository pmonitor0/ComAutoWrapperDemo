<Window x:Class="ComAutoWrapperDemo.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="COM AutoWrap Demo" Height="300" Width="400" Initialized="Window_Initialized">
    <Grid Margin="10">
        <StackPanel>
            <Button Content="Start Excel" Click="StartExcel_Click" Margin="5"/>
            <Button Content="Add Workbook" Click="AddWorkbook_Click" Margin="5"/>
            <Button Content="Quit Excel" Click="QuitExcel_Click" Margin="5"/>
            <TextBox Name="LogBox" Margin="5" Height="150" TextWrapping="Wrap" VerticalScrollBarVisibility="Auto" AcceptsReturn="True"/>
        </StackPanel>
    </Grid>
</Window>
